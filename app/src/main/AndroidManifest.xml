<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.habitstracker">

    <application
        android:allowBackup="true"
        android:icon="@drawable/appicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".Fragment_w1"></activity>
        <activity android:name=".Fragment_w2"></activity>
        <activity android:name=".Fragment_w3"></activity>
        <activity android:name=".Fragment_today"></activity>
        <activity android:name=".Fragment_list"></activity>
        <activity android:name=".Walkthrough"></activity>
        <activity android:name=".HabitsAdd"></activity>
        <activity android:name=".HabitsUpdate"></activity>
        <activity android:name=".Fragment_info"></activity>
        <receiver
            android:name=".AlarmReceiver"
            android:enabled="true"
            android:exported="false" />
    </application>


</manifest>